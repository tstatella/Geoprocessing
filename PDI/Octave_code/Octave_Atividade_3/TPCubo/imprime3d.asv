function imprime3d(objeto)

global CUBO

figure,plot3(CUBO(1,1),CUBO(1,2),CUBO(1,3),'r',...
             CUBO(2,1),CUBO(2,2),CUBO(2,3),'g',...
             CUBO(3,1),CUBO(3,2),CUBO(3,3),'b',...
             CUBO(4,1),CUBO(4,2),CUBO(4,3),'c',...
             CUBO(5,1),CUBO(5,2),CUBO(5,3),'m',...
             CUBO(6,1),CUBO(6,2),CUBO(6,3),'y'),...
             grid on, xlabel('X'),ylabel('Y'),zlabel('Z');
end